NUMBER=           # Número de la línea donde se instala el receptor de llamadas
WEBHOOK_URL=      # Url del servicio en el cual se registra la llamada
PORT='/dev/ttyACM0' # Puerto por donde se conecta el modem
BAUD=115200         # Velocidad de transmisión en bits por segundo
MAX_RINGS=3         # Cantidad de timbres antes de contestar
AUDIO_FILE=voices/busy_lines.wav
COUNTRY_CODE=598    # Código de país para normalización (Uruguay)
TRUNK_PREFIX=0      # Prefijo troncal a remover (ej. 0)
PLAY_AUDIO=1        # 1/true para reproducir audio; 0/false para colgar sin audio
HANGUP_DELAY_MS=1200
VSM_CODEC=130       # 130=mulaw, 129=alaw, 128=pcm unsigned 8-bit
SAMPLE_RATE=8000    # Hz